<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
 <locale-config> 
     <default-locale>en</default-locale> 
     <supported-locale>ar</supported-locale>
</locale-config>
<resource-bundle>
<base-name>com.cme.messages.messages</base-name>
<var>msgs</var> 
</resource-bundle>
<resource-bundle>
<base-name>com.cme.messages.menu</base-name>
<var>mnuMsg</var> 
</resource-bundle>
<resource-bundle>
<base-name>com.cme.messages.compProfile</base-name>
<var>compPMsg</var> 
</resource-bundle>

<resource-bundle>
<base-name>com.cme.messages.langMast</base-name>
<var>langMsg</var> 
</resource-bundle>

</application>

</faces-config>
